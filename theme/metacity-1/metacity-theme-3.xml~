<!-- drawing operations -->

	<!-- title -->

<draw_ops name="title_focused">
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 2"
               color="#141414" />
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               color="#333333" />
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               color="blend/gtk:fg[NORMAL]/#C8CC80/0.3" />

	<title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width / 2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 2"
               ellipsize_width="width"
               color="#141414" />
	<title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width / 2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               ellipsize_width="width"
               color="#333333" />
	<title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width / 2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               ellipsize_width="width"
               color="blend/gtk:fg[NORMAL]/#C8CC80/0.3" />
</draw_ops>

<draw_ops name="title_unfocused">
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 2"
               color="#222222" />
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               color="#282828" />
	<title version="< 3.1"
               x="(0 `max` ((width - title_width) / 2)) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               color="C_title_unfocused"/>

    <title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width / 2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 2"
               ellipsize_width="width"
               color="#222222" />
	<title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width / 2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2))"
               ellipsize_width="width"
               color="#282828" />
	<title version=">= 3.1"
               x="(0 `max` ((frame_x_center - title_width/2) `min` (width - title_width))) + 2"
               y="(0 `max` ((height - title_height) / 2)) + 1"
               ellipsize_width="width"
               color="C_title_unfocused"/>
</draw_ops>

